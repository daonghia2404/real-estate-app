doctype html
html
  head
    meta(charset="utf-8")
    meta(name="viewport" content="width=device-width, initial-scale=1.0")
    meta(http-equiv="X-UA-Compatible" content="ie=edge")
    title Real Estate App

    //- Bootstrap CSS
    link(rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/css/bootstrap.min.css")

    //- OwlCarousel CSS
    link(rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/owl.carousel@2.3.4/dist/assets/owl.carousel.min.css")

    //- Style CSS
    link(rel="stylesheet" type="text/css" href="./assets/css/main.css")

    block head

  body.layout-dark
    include header-dark.pug
    include header-nav-dark.pug 
    .main-dark
      block pages
    
    include footer-dark.pug
    //- jQuery
    script(src="https://cdn.jsdelivr.net/npm/jquery@3.6.0/dist/jquery.min.js") 

    //- Bootstrap JS
    script(src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/js/bootstrap.min.js") 

    //- OwlCarousel JS
    script(src="https://cdn.jsdelivr.net/npm/owl.carousel@2.3.4/dist/owl.carousel.min.js") 

    //- Chart JS
    script(src="https://cdn.jsdelivr.net/npm/chart.js@2.9.4/dist/Chart.min.js")

    block scripts

    //- Java Script
    script(src="./assets/js/main.js")
